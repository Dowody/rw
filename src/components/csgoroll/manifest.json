{
    "manifest_version": 3,
    "name": "CSGORoll Cookie Extractor",
    "version": "1.0",
    "description": "Extract session cookies from CSGORoll domains",
    "permissions": ["cookies", "activeTab"],
    "host_permissions": [
      "*://*.csgoroll.com/*",
      "*://*.csgorolltr.com/*",
      "*://*.csgoroll.gg/*"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }